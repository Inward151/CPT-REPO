
public class Test {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		//Change i-- to i++. In order to count
		 for(int i = 0; i < 10; i++) {
	            System.out.println("The value if i is: " + i);
	        }

	        System.out.println("Program has completed successfully!");
	    
		
		

	}
}
